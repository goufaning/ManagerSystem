<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <filter>
        <filter-name>CharacterEncodingFilter</filter-name>
        <filter-class>com.goufaning.system.filter.CharacterEncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CharacterEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>PrepareServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.PrepareServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrepareServlet</servlet-name>
        <url-pattern>/PrepareServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InWarehoustServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.InWarehoustServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InWarehoustServlet</servlet-name>
        <url-pattern>/InWarehoustServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OutWarehouseServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.OutWarehouseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OutWarehouseServlet</servlet-name>
        <url-pattern>/OutWarehouseServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InventoryServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.InventoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InventoryServlet</servlet-name>
        <url-pattern>/InventoryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InBaobiaoServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.InBaobiaoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InBaobiaoServlet</servlet-name>
        <url-pattern>/InBaobiaoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OutBaobiaoServlet</servlet-name>
        <servlet-class>com.goufaning.system.servlet.OutBaobiaoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OutBaobiaoServlet</servlet-name>
        <url-pattern>/OutBaobiaoServlet</url-pattern>
    </servlet-mapping>


</web-app>